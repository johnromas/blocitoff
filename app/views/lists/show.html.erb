<h2><%= @list.name %></h2>

<% @todos.each do |todo| %>
  <h4><%= todo.desc %></h4>
  <% end %>


    




<%= link_to "Add Todo's", new_list_todo_path(@list), class: 'btn btn-success' %> 
<%= link_to "Delete List", @list, method: :delete, class: 'btn btn-danger', confirm: 'Are you sure you want to delete this list?' %> 
